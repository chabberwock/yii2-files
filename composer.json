{
    "name": "chabberwock/yii2-files",
    "description": "Module to handle async file uploads",
    "keywords": [
        "yii",
        "yii2",
        "yii 2",
        "upload",
        "files"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/chabberwock/yii2-files",
    "authors": [
        {
            "name": "Alexandr Makarov",
            "email": "notengine@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Originsoft",
            "email": "contact@originsoft.org",
            "role": "Company"
        }
    ],
    "require": {
        "2amigos/yii2-file-upload-widget": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "chabberwock\\files\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
